# このファイルを .env にコピーして値を設定してください
# cp env.example .env

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key

# GitHub API（クローラー用。複数トークンをカンマ区切りで指定）
GITHUB_TOKENS=ghp_token1,ghp_token2,ghp_token3

# API設定
API_SECRET_KEY=your-secret-key-for-jwt
ADMIN_API_KEY=your-admin-key-for-management

# クローラー設定
CRAWL_MAX_SERVERS=500
HEALTH_CHECK_INTERVAL_MIN=60
HEALTH_CHECK_TIMEOUT_SEC=10
HEALTH_CHECK_CONCURRENCY=20
